        <KeyFragment ref="message.uri" />
    </CacheKey>
	<CacheResource>book-api-v1-response-cache</CacheResource>
    <ExpirySettings>
        <TimeoutInSec ref="config.cacheEntryExpiry">120</TimeoutInSec>
    </ExpirySettings>
</ResponseCache>
